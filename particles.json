{
  "particles": {
    "number": {
      "value":150
    },
    "color": {
      "value": "random"
    },
    "shape": {
      "type": "image",
      "image": {
        "src": "panda-face.png"
      }
    },
    "opacity": {
      "value": 0.9,
      "random": true
    },
    "size": {
      "value": 25,
      "random": true
    },
