        "mode": "push"
      }
    },
    "modes": {
      "grab": {
        "distance": 250,
        "line_linked": {
          "opacity": 0.5
        }
      },
      "bubble": {
        "distance": 100,
        "size": 80,
        "duration": 2,
        "opacity": 0.8,
        "speed": 3
      },
      "repulse": {
        "distance": 150,
        "duration": 5
      },
      "push": {
        "particles_nb": 1
      }
    }
  }
